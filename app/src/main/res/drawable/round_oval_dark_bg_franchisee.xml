<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="3px"
        android:left="3px"
        android:right="3px"
        android:top="3px">
        <shape
            android:padding="10dp"
            android:shape="oval">
            <solid android:color="@color/icon_round_shape_fr" />
            <stroke
                android:width="1dp"
                android:color="@color/gray" />
            <corners
                android:bottomLeftRadius="5dp"
                android:bottomRightRadius="5dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="40dp"
        android:drawable="@drawable/logo_with_shadow"
        android:left="40dp"
        android:right="40dp"
        android:top="40dp" />
</layer-list>